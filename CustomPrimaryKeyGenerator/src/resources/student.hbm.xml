<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="beans.Student" table="student" schema="learninghibernate">
		<id name = "UID">
			<generator class="beans.StudentUIDCustomGenerator"></generator>
		</id>
		<property name="name"/>
		<property name="email"/>
		<property name="address"/>
	</class>
</hibernate-mapping>